{
  "name": "wikiwalk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/scss/styles.scss -o src/styles/css --include-path ./src --include-path ./node_modules",
    "watch-css": "npm run buildcss &&& node-sass-chokidar src/styles/scss/styles.scss -o src/styles/css --watch --recursive --include-path ./src --include-path ./node_modules",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
